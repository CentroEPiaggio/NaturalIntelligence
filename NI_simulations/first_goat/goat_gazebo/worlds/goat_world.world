<?xml version="1.0" ?>
<sdf version="1.4">
  <!-- We use a simple world for the goat robot so that the camera angle is launched correctly -->

  <world name="default">

    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

  </world>
</sdf>